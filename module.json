{
    "id": "lancer-automations",
    "title": "Lancer Automations",
    "version": "2.1.5",
    "description": "Big ol' module containing many automations for lancer.",
    "socket": true,
    "authors": [
        {
            "name": "Agraael"
        }
    ],
    "url": "https://github.com/Agraael/lancer-automations",
    "manifest": "https://github.com/Agraael/lancer-automations/releases/latest/download/module.json",
    "download": "https://github.com/Agraael/lancer-automations/releases/latest/download/module.zip",
    "esmodules": [
        "scripts/overwatch.js",
        "scripts/main.js"
    ],
    "styles": [
        "styles/codemirror-dark.css",
        "styles/interactive-tools.css"
    ],
    "compatibility": {
        "minimum": "12",
        "verified": "12"
    },
    "relationships": {
        "systems": [
            {
                "id": "lancer",
                "type": "system"
            }
        ],
        "requires": [
            {
                "id": "lancer-style-library",
                "type": "module"
            },
            {
                "id": "temporary-custom-statuses",
                "type": "module"
            }
        ],
        "optional": [
            {
                "id": "_CodeMirror",
                "type": "module"
            }
        ]
    }
}